{
  "name": "eclipse-engine",
  "version": "1.0.0",
  "description": "A Game Engine using Electron, Javascript and the HTML5 Canvas feature",
  "main": "index.js",
  "author": {
    "name": "N1ghtTheF0x",
    "email": "kil.night.prower@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "discord-rpc": "^3.1.4",
    "discord.js": "^12.5.1"
  },
  "devDependencies": {
    "electron": "11.2.1",
    "electron-builder": "22.9.1"
  },
  "scripts": {
    "start": "npx electron .",
    "debug": "npx electron . --edev -edebug",
    "server": "npx electron . --server",
    "build": "npx electron-builder",
    "hardware": "npx electron . --hardware",
    "help": "npx electron . --help"
  },
  "build": {
    "productName": "Eclipse Engine",
    "copyright": "Copyright Â© 2020 N1ghtTheF0x",
    "appId": "org.eclipseteam.ee",
    "win": {
      "icon": "./textures/common/icon.ico",
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "repository": {
    "url": "https://github.com/N1ghtTheF0x/Eclipse-Engine",
    "type": "git"
  },
  "homepage": "https://github.com/N1ghtTheF0x/Eclipse-Engine#readme",
  "keywords": [
    "game engine",
    "html5",
    "webgl",
    "canvas",
    "electron"
  ],
  "bugs": {
    "email": "kilo.night.prower@gmail.com",
    "url": "https://github.com/N1ghtTheF0x/Eclipse-Engin/issues"
  },
  "browser": true,
  "os": [
    "win32",
    "!darwin",
    "!linux"
  ],
  "cpu": [
    "x64",
    "ia32",
    "!arm"
  ]
}
